<?xml version="1.0" encoding="UTF-8"?>
<config>
	<!-- 
		Commands allow you to pass the exe an action through the command line.  
		ie CSIHelp.exe command=wired_help will perform the wired_help command after Help.exe loads
		targetName - the name of the action to perform
	-->
	<commands>
		<command targetName="wireless_help">wireless_help</command>
		<command targetName="wired_help">wired_help</command>
		<command targetName="wps">wps</command>
	</commands>
	
	<!--
		name - The name used to refer to this action.
		animationType - AS2|AS3 - Tells the viewer which communication model to use when passing the language to the animation.
	 -->
	<targets>
		<target name="wireless_help">assets/wireless_help.swf</target>
		<target name="wired_help">assets/wired_help.swf</target>
		<target name="wps">assets/wps.swf</target>
	</targets>
		
	
	<!-- 
		List of language codes and their associated Primary Laguage Identifiers.
		For the complete list see http://msdn.microsoft.com/en-us/library/ms776294%28VS.85%29.aspx#NotesPrim
		Also check http://msdn.microsoft.com/en-us/library/system.globalization.cultureinfo(VS.71).aspx
		For Chinese check http://msdn.microsoft.com/en-us/library/system.globalization.cultureinfo.parent(VS.80).aspx
		This list also defines the list of available languages.  Any language that is not in this list will display EN.
		The language codes must *not* contain WW.
		The language codes must be upper case.
		All animations must check the language against their own list of known languages.
	-->
	<languageCodes>
		<language name="AR">01</language>
		<language name="CA">03</language>
